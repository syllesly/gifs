class Rect {
  public float x=0;
  public float y=0;
  public float w=0;
  public float h=0;
  public float startX=0;
  public float startY=0;
  public float startW=0;
  public float startH=0;
  public Rect(float getX, float getY, float getW, float getH) {
    x=getX;
    y=getY;
    w=getW;
    h=getH;
    startX=getX;
    startY=getY;
    startW=getW;
    startH=getH;
  }
}